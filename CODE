#include<stdio.h>

int main(){
    int a,b,c,d;
    int n;
    scanf("%d",&n);
    while(!n==0)
    {
    scanf("%d %d %d",&a,&b,&c);
    if(c>=a&&a>=b)
        {int t;
        t=a;
        a=c;
        c=b;
        b=t;}
    if(c>=b&&b>=a)
        {int w;
        w=a;
        a=c;
        c=w;}
    if(b>=a&&a>=c)
        {int t;
        t=a;
        a=b;
        b=t;}
    if(b>=c&&c>=a)
        {int t;
        t=a;
        a=b;
        b=c;
        c=t;}
    if(a>=c&&c>=b)
        {int t;
        t=b;
        b=c;
        c=t;}
    d=(c*c)+(b*b)-(a*a);
    
    if(a>0&&b>0&&c>0&&((a+b)>c)&&((b+c)>a)&&((a+c)>b)&&(d=0))
    {
        printf("YES\n");
        printf("Right\n");
    }
    else if(a>0&&b>0&&c>0&&((a+b)>c)&&((b+c)>a)&&((a+c)>b)&&(d>0))
    {
        printf("YES\n");
        printf("Acute\n");
    }
    else if(a>0&&b>0&&c>0&&((a+b)>c)&&((b+c)>a)&&((a+c)>b)&&(d<0))
    {
        printf("YES\n");
        printf("Obtuse\n");
    }
    else
    printf("NO\n");
    n--;
    }
    
    return 0;
}
